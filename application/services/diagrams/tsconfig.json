{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "pretty": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "target": "es6",
        "outDir": "./dist",
        "baseUrl": "./lib"
    },
    "include": [
        "src/**/*.ts", "src/models/NodeModels.ts",
    ],
    "exclude": [
        "node_modules"
    ],
    "glob": "./src/**/!(*.d|*.spec).ts",
    "modifiers": false,
    "nomnoml": ["#arrowSize: 1.5","#.interface: fill=#8f8 dashed"],
    "outFile":"./src/**/*.svg"
}